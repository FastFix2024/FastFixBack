<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="create_table_car_details" author="dev_fastfix">
        <createTable tableName="car_details">

            <column name="id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>

            <column name="fuel_type" type="varchar(255)"/>

            <column name="last_maintenance_date" type="date"/>

            <column name="insurance_company_id" type="bigint"/>

        </createTable>

    </changeSet>


</databaseChangeLog>