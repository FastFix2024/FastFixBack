<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">

    <changeSet id="create_foreign_keys_users_bookmarks" author="ffdev">

        <addForeignKeyConstraint baseTableName="users_bookmarks"
                                 baseColumnNames="user_id"
                                 constraintName="fk_users_bookmarks_to_users"
                                 referencedTableName="users"
                                 referencedColumnNames="id"/>

        <addForeignKeyConstraint baseTableName="users_bookmarks"
                                 baseColumnNames="bookmarks_id"
                                 constraintName="fk_users_bookmarks_to_bookmarks"
                                 referencedTableName="bookmarks"
                                 referencedColumnNames="id"/>

    </changeSet>

</databaseChangeLog>